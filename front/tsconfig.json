{
  "compilerOptions": {
    "paths": {
      "*": [
        "types/*"
      ]
    },
    "baseUrl": ".",
    "target": "es2018",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "sourceMap": true,
    "jsx": "react",
    "strictNullChecks": true
  }
}
